  {
    "manifest_version": 2,
    "name": "abrir sessoes rapidamente",
    "description": "abre sites que uso em um click",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
      },
    "permissions": [
      "contextMenus",
      "tabs",
      "activeTab",
      "declarativeContent",
      "storage"
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
    
    
  }
